    // 使用できる言語とそのレベルを処理するロジックを追加\n  };\n\n  const handleBioChange = (e) => {\n    setBio(e.target.value);\n  };\n\n  const handleHourlyRateChange = (e) => {\n    setHourlyRate(e.target.value);\n  };\n\n  const handleSignUp = () => {\n    // フォームの値をまとめてリクエストに含めて送信するロジックを実装\n    const formData = {\n      displayName,\n      email,\n      password,\n      age,\n      gender,\n      occupation,\n      profileImage,\n      shiftSchedule,\n      languages,\n      bio,\n      hourlyRate,\n    };\n\n    // サーバーにデータを送信するためのAPI呼び出しを行うか、適切な方法を選択\n  };\n\n  return (\n    <div>\n      <h2>会員登録</h2>\n      <div>\n        <label>表示名</label>\n        <input type=\"text\" placeholder=\"表示名\" value={displayName} onChange={handleDisplayNameChange} />\n      </div>\n      <div>\n        <label>メールアドレス</label>\n        <input type=\"email\" placeholder=\"メールアドレス\" value={email} onChange={handleEmailChange} />\n      </div>\n      <div>\n        <label>パスワード</label>\n        <input type=\"password\" placeholder=\"パスワード\" value={password} onChange={handlePasswordChange} />\n      </div>\n    \n      {/* 年齢 */}\n      <div>\n        <label>年齢</label>\n        <input type=\"number\" placeholder=\"年齢\" value={age} onChange={handleAgeChange} />\n      </div>\n    \n      {/* 性別 */}\n      <div>\n        <label>性別</label>\n        <select value={gender} onChange={handleGenderChange}>\n          <option value=\"male\">男性</option>\n          <option value=\"female\">女性</option>\n          <option value=\"other\">その他</option>\n        </select>\n      </div>\n    \n      {/* 職業 */}\n      <div>\n        <label>職業</label>\n        <input type=\"text\" placeholder=\"職業\" value={occupation} onChange={handleOccupationChange} />\n      </div>\n    \n      {/* プロフィール画像 */}\n      <div>\n        <label>プロフィール画像</label>\n        <input type=\"file\" accept=\"image/*\" onChange={handleProfileImageChange} />\n      </div>\n    \n      {/* 1週間のシフトスケジュール */}\n      <div>\n        <label>1週間のシフトスケジュール</label>\n        <textarea placeholder=\"1週間のシフトスケジュール\" value={shiftSchedule} onChange={handleShiftScheduleChange}></textarea>\n      </div>\n    \n      {/* 使用できる言語とそのレベル */}\n      {/* 例: 星をタップできるレベルの選択を実装 */}\n    \n      {/* 自己紹介文 */}\n      <div>\n        <label>自己紹介文</label>\n        <textarea placeholder=\"自己紹介文\" value={bio} onChange={handleBioChange}></textarea>\n      </div>\n    \n      {/* 時給 */}\n      <div>\n        <label>時給</label>\n        <input type=\"number\" placeholder=\"時給\" value={hourlyRate} onChange={handleHourlyRateChange} />\n      </div>\n    \n      {/* 記載欄 */}\n      {/* 任意の追加情報を入力するためのフィールド */}\n    \n      <div>\n        <button onClick={handleSignUp}>登録</button>\n      </div>\n    </div>\n  );\n}\n\nexport default AuthForm;\n","/Users/h_sato/Desktop/Best_Day_in_Japan/BestDayinJapan_front/src/components/GuestPage.js",[],[],"/Users/h_sato/Desktop/Best_Day_in_Japan/BestDayinJapan_front/src/components/GuestRegister.js",["103"],[],"/Users/h_sato/Desktop/Best_Day_in_Japan/BestDayinJapan_front/src/components/GuideProfileCard.js",[],[],"/Users/h_sato/Desktop/Best_Day_in_Japan/BestDayinJapan_front/src/components/GuideProfile.js",["104"],[],"/Users/h_sato/Desktop/Best_Day_in_Japan/BDIJ_front/src/index.js",[],[],"/Users/h_sato/Desktop/Best_Day_in_Japan/BDIJ_front/src/App.js",[],[],"/Users/h_sato/Desktop/Best_Day_in_Japan/BDIJ_front/src/reportWebVitals.js",[],[],"/Users/h_sato/Desktop/Best_Day_in_Japan/BDIJ_front/src/components/GuidePage.js",[],[],"/Users/h_sato/Desktop/Best_Day_in_Japan/BDIJ_front/src/components/GuestPage.js",[],[],"/Users/h_sato/Desktop/Best_Day_in_Japan/BDIJ_front/src/components/Home.js",[],[],"/Users/h_sato/Desktop/Best_Day_in_Japan/BDIJ_front/src/components/GuideProfile.js",["105"],[],"/Users/h_sato/Desktop/Best_Day_in_Japan/BDIJ_front/src/components/AuthForm.js",["106","107","108","109"],[],"/Users/h_sato/Desktop/Best_Day_in_Japan/BDIJ_front/src/components/GuideProfileCard.js",[],[],{"ruleId":"110","severity":1,"message":"111",